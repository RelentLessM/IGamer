@{
    this.ViewData["Title"] = "Admin dashboard";
}

<section class="breadcrumb breadcrumb_bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb_iner text-center">
                    <div class="breadcrumb_iner_item">
                        <h2>@ViewData["Title"]</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="container">
    <div class="section_padding">
        <div class="progress-table">
            <div class="table-head">
                <div class="serial"></div>
                <div class="country">Section</div>
                <div class="percentage"></div>
                <div class="visit">Go To</div>
            </div>

            <div class="table-row">
                <div class="serial"></div>
                <div class="country">Posts</div>
                <div class="percentage"></div>
                <div class="visit">
                    <a type="button" class="swiper-button-black primary text-white btn_1 circle large" asp-area="Administration" asp-controller="Posts" asp-action="AllPosts">See Posts</a>
                </div>
            </div>
            <div class="table-row">
                <div class="serial"></div>
                <div class="country">Guides</div>
                <div class="percentage"></div>
                <div class="visit">
                    <a type="button" class="swiper-button-black primary text-white btn_1 circle large" asp-area="Administration" asp-controller="Guides" asp-action="AllGuides">See guides</a>
                </div>
            </div>
            <div class="table-row">
                <div class="serial"></div>
                <div class="country">Suggestions</div>
                <div class="percentage"></div>
                <div class="visit">
                    <a type="button" class="swiper-button-black primary text-white btn_1 circle large" asp-area="Administration" asp-controller="Suggestions" asp-action="All">Suggestions</a>
                </div>
            </div>
        </div>
    </div>
</div>